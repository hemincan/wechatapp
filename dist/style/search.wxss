.search_read_only {
  align-items: center;
  display: flex;
  margin: 0 auto;
  width: 100%;
}
.search_read_only .search_content {
  display: flex;
  align-items: center;
  border-radius: 20rpx;
  color: #999;
  width: 90%;
  background: #efefef;
  padding: 0 10rpx;
  height: 65rpx;
  margin: 0 auto;
}
.search_read_only .search_content .search_input {
  font-size: 26rpx;
  width: 100%;
}
.search_read_only .icon-search {
  font-size: 40rpx;
}
.search_read_only .icon-message {
  font-size: 50rpx;
}
.top_search {
  padding: 10rpx 0rpx;
  background: #fff;
}
.spacing {
  height: 20rpx;
  width: 100%;
  overflow: hidden;
  background-color: #f5f5f5;
}
